\begin{enumerate}
\item -20 eV; singlet
\item -12 eV; degeneracy of 4
\item ???
\item $\Psi^{\text{HF}} = \frac{1}{\sqrt{2}}(\phi_1(x_1)
  \alpha(x_1)\phi_2(x_2)\beta(x_2) - \phi_2(x_1)\beta(x_1)
  \phi_1(x_2)\alpha(x_2))$ \\
  spatial orbitals $\phi_1(x)$ and $\phi_2(x)$ with
  spin $\alpha$ and $\beta$ for electrons $x_1$ and $x_2$
\item $\hat{H} = \sum_i^3 - \frac{1}{2}\nabla_i^2
  -\sum_i^3\frac{3}{|r_i|} + \sum_i^3\sum_{j\neq i}^3
  \frac{1}{2|r_i-r_j|}$
\item $J = 1$, $M = \{-1,0,1\}$, a total of $2J+1 = 3$ states
\item ???
\item $E_{\text{IP}} = |E_N^{\text{He}} - E_{N-1}^{\text{He}}|
  = |-77.9 + 54.4| = 23.5$ eV
\item $5.6\%$ Error
\item From lowest to highest in energy: $^4S$, $^2D$, $^2P$
\end{enumerate}
  
  
  
