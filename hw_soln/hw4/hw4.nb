  FormBox[
   RowBox[{
    SubscriptBox["E", 
     RowBox[{"\[Nu]", ",", "n"}]], ","}], TraditionalForm]],
  FormatType->"TraditionalForm",ExpressionUUID->
  "d799affd-93af-4f4a-93b9-faf9e4346f15"],
